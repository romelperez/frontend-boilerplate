    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.11.6",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.1",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cssnano": "^3.7.3",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "i18next": "^3.4.1",
    "jquery": "^3.1.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "materialize-css": "^0.98.2",
    "moment": "^2.18.1",
