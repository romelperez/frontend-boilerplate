    "require-dir": "^0.3.0",
    "string": "^3.3.1",
    "uuid": "^3.1.0",
    "validator": "^6.3.0",
    "vulcanval": "^3.1.10",
    "webpack": "^1.15.0",
    "winston": "^2.3.1"
